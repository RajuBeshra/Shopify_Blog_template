      @include respond-to('medium') {
        font-size: 1.2em;
        line-height: 1em;
        margin-bottom: 10px;
      }
    }

    .banner-section__caption-button {
      width: auto;
      line-height: 0;
      padding: 20px 25px;
      pointer-events: all;

      @include respond-to('medium') {
        padding: 15px;
        font-size: 12px;
      }
    }
