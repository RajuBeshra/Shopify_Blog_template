/* Variables */

$table-border: 1px solid #cccccc;
$table-border-color: #e0e0e0;
$table-header-background: #ededed;
$table-data-background: #fbfbfb;
$table-data-background-even: #f7f7f7;
$table-data-background-hover: #f1f1f1;

/* Collapsible tables */

.collapsible-table {
  width: 100%;
  margin-bottom: 20px;
  border: $table-border;

  @include respond-to('medium') {
    border: none;
  }

  a {
    color: {{ settings.link_color }};
    font-weight: bold;

    &:hover,
    &:active {
      color: {{ settings.link_hover_color }};
      text-decoration: underline;
    }
  }

  @include respond-to('medium') {
    thead {
      display: none;
    }
  }

  th {
    padding: 12px;
    font-weight: bold;
    border-bottom: 1px solid $table-border-color;
    background-color: $table-header-background;
  }

  tr {
    text-align: center;

    @include respond-to('medium') {
      display: block;
      text-align: left;
      margin-bottom: 20px;
      border: 1px solid $table-border-color;
    }

    &.even td {
      background-color: $table-data-background-even;

      @include respond-to('medium') {
        background-color: $table-data-background;
      }
    }

    @include respond-to('medium') {
      &:last-child {
        margin-bottom: 0;
      }
    }

    td {
      padding: 12px;
      position: relative;
      border-bottom: 1px solid $table-border-color;
      border-left: 1px solid $table-border-color;
      background-color: $table-data-background;

      @include respond-to('medium') {
        display: block;
        @include flexbox();
        @include align-items(center);
        @include justify-content(space-between);
        padding: 0 10px 0 0;
        border: none;
        border-bottom: 1px solid $table-border-color;
      }

      @include respond-to('medium') {
        &:before {
          content: attr(data-label);
          z-index: 1;
          width: 45%;
          font-weight: bold;
          padding: 4px 0px 4px 10px;
          word-break: break-word;
        }
      }

      @include respond-to('medium') {
        &:after {
          content: "";
          top: 0;
          left: 0;
          z-index: 0;
          min-width: 48%;
          min-height: 100%;
          position: absolute;
          border-right: 1px solid $table-border-color;
          background-color: $table-header-background;
        }
      }

      @include respond-to('medium') {
        &:last-child {
          border-bottom: none;
        }

        &:first-child {
          text-align: left;
          border-left: 0;
        }
      }

      &.value input {
        margin-bottom: 0;
      }
    }

    &:hover td {
      background-color: $table-data-background-hover;

      @include respond-to('medium') {
        background-color: $table-data-background;
      }
    }
  }

  tfoot {
    @include respond-to('medium') {
      display: block;
      margin-top: 20px;

      tr {
        margin-bottom: 0;
        border-bottom: none;

        &:last-child {
          border-bottom: 1px solid $table-border-color;
        }
      }
    }
  }

  .order-details__total,
  .order-details__discount {
    span {
      display: block;
    }

    @include respond-to('medium') {
      text-align: right;
      width: 45%;
    }

    .order-details__discount {
      @include respond-to('medium') {
        width: 100%;
      }
    }
  }

  @include respond-to('medium') {
    .order-details__product {
      width: 45%;
      text-align: right;
    }

    .order_summary {
      td {
        border-bottom: none;
      }

      .order_summary__discount {
        text-align: right;
      }
    }
  }
}

