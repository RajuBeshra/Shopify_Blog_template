li {
  line-height: 1.8em;
  margin-bottom: 12px;
}

ul.large li { line-height: 1.8em; }

ul.none {
  list-style: none outside;
  margin-left: 0;
}

ul.none li {
  list-style: none;
  padding: 0;
}

li p { line-height: 1.8em; }

ul { line-height: 1.5; }

ul li {
  list-style-type: disc;
  list-style-position: inside;
  padding: 6px 0 6px 0;
  margin-left: 0;
  margin-bottom: 0;
